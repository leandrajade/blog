
<table>
    <thead>
        <tr>
            <th>count</th>
            <th>ID</th>
            <th>Name</th>
            <th>Body</th>
        </tr>
    </thead>
    <tbody>
        <% counter = 1 %>
        <tr>
            <td><%= counter %></td>
            <td><%= @article.id %></td>
            <td><%= @article.name %></td>
            <td><%= @article.body %></td>
        </tr>
        <% counter += 1%>
    </tbody>
</table>
